.music-head {
  position: relative;
  height: 75px;
  width: 100%;
  font-size: 16px;
  line-height: 80px;
  text-align: center;
}
.music-head .music-head-icon {
  position: absolute;
  left: 8px;
  top: 24px;
  width: 90px;
  height: 30px;
  background-color: rgba(0, 0, 0, 0.4);
  color: white;
  line-height: 30px;
  border-radius: 15px;
  display: flex;
  justify-content: space-evenly;
}
.music-head .music-head-iconblack {
  color: black;
  border: 1px #eaeaea solid;
  background: #fff;
}

